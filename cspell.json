{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"import": ["@cspell/dict-pl_pl/cspell-ext.json"],
	"ignorePaths": ["pnpm-lock.yaml", "cspell.json", "**.svg", ".git/", ".gitignore", ".vscode/"],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": ["codeowner", "nojs", "rebeccapurple", "rolldown", "stylex", "stylexjs", "typecheck", "vite", "vitejs"],
	"enableGlobDot": true,
	"ignoreWords": [
		"commonality",
		"commonalityco",
		"lingui",
		"nuqs",
		"pawelblaszczyk",
		"predev",
		"taze",
		"tseslint",
		"xyflow"
	],
	"overrides": [{ "filename": "apps/lingui/src/locales/pl-PL.po", "language": "pl-PL,en-US" }]
}
